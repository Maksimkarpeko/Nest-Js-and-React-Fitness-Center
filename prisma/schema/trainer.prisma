model Trainer {
  trainerId       Int       @id @default(autoincrement()) @map("trainer_id")
  firstName       String    @map("first_name")
  secondName      String    @map("second_name")
  specialization  String
  experienceYears Float     @map("experience_years")
  phone           String    @unique
  email           String    @unique
  countWorkday    Int       @map("count_work_day")
  status          GlobalStatus
  workouts        Workout[]
}
